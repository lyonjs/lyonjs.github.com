{
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all",
  "ignorePatterns": [
    "/node_modules",
    "/.pnp",
    ".pnp.js",
    "pnpm-lock.yaml",
    "/coverage",
    "/.next/",
    "/out/",
    "dist",
    "/build",
    ".DS_Store",
    "*.pem",
    "npm-debug.log*",
    "yarn-debug.log*",
    "yarn-error.log*",
    ".pnpm-debug.log*",
    ".env*.local",
    ".vercel",
    "*.tsbuildinfo"
  ]
}
